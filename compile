set -v
gcc -c -Iinclude src/pl32-memory.c src/pl32-shell.c
ar rc libpl32.a *.o
gcc -c -fPIC -Iinclude src/pl32-memory.c src/pl32-shell.c
gcc -shared *.o -o libpl32.so
gcc --static -Iinclude tests/pl32-memory-test.c *.o -o pl32-memory-test.out
gcc --static -Iinclude tests/pl32-shell-test.c *.o -o pl32-shell-test.out
rm *.o
